{
  "name": "canalmedico-backend",
  "version": "1.0.1",
  "description": "Backend API para CanalMedico - Plataforma de consultas médicas asíncronas",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc && tsc-alias",
    "start": "node dist/server.js",
    "start:migrate": "npx prisma migrate deploy && node dist/server.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "prisma:deploy": "prisma migrate deploy",
    "prisma:seed": "tsx prisma/seed.ts",
    "railway:deploy": "npm run build && prisma migrate deploy && npm start",
    "test": "jest",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "e2e:phase2.2": "tsx scripts/e2e-phase-2-2.ts",
    "verify:railway": "tsx scripts/verify-railway.ts",
    "verify:railway-deploy": "tsx scripts/verify-railway-deploy.ts",
    "verify:health": "tsx scripts/verify-health.ts",
    "verify:remote-health": "tsx scripts/verify-remote-health.ts",
    "debug:port": "tsx scripts/print-port.ts",
    "whatsapp:test": "tsx scripts/whatsapp_smoke_test.ts",
    "system:test": "tsx scripts/system_smoke_test.ts"
  },
  "keywords": [
    "medical",
    "chat",
    "consultation",
    "telemedicine"
  ],
  "author": "CanalMedico",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.490.0",
    "@aws-sdk/s3-request-presigner": "^3.937.0",
    "@prisma/client": "^5.7.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.6",
    "@types/node-cron": "^3.0.11",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "axios": "^1.13.2",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "firebase-admin": "^12.0.0",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "mercadopago": "^2.10.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^4.2.1",
    "prisma": "^5.7.1",
    "socket.io": "^4.5.4",
    "stripe": "^14.10.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1"
  },
  "engines": {
    "node": ">=18.0.0 <=20.0.0",
    "npm": ">=9.0.0"
  }
}
